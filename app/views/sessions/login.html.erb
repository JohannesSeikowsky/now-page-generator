
<!-- link to login -->
<div class="link_to_login">
  <%= link_to "Login", login_path %>
</div>

<!-- content -->
<div class="container_normal">
  <h1> 2- login </h1>
  <%= form_for :login_provided, url: login_path do |f| %>
    <h4> Your Email: </h4>
    <%= f.text_field :email, placeholder: "email" %> <br>
    <h4> Your Password: </h4>
    <%= f.password_field :password, placeholder: "password" %> <br>
    <%= f.submit "Login." %>
  <% end %>
</div>