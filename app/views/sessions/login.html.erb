<!-- header -->
<div class="simple_header">
  <%= link_to "Signup", signup_path %>
</div>

<!-- content -->
<div class="simple_container">
  <h2> Login </h2>
  <%= form_for :login, url: login_action_path do |f| %>
    <h4> Your Email: </h4>
    <%= f.text_field :email_provided, placeholder: "email" %> <br>
    <h4> Your Password: </h4>
    <%= f.password_field :password_provided, placeholder: "password" %> <br>
    <%= f.submit "Login." %>
  <% end %>
</div>